{
	"name": "ccsp-prototype-ui",
	"version": "1.0.0",
	"description": "Prototype for CSSP",
	"main": "",
	"dependencies": {
		"angular": "^1.8.0",
		"angular-animate": "^1.8.0",
		"angular-datatables": "^0.6.4",
		"angular-resource": "^1.8.0",
		"angular-route": "^1.8.0",
		"lodash": "^4.17.19",
		"moment": "^2.29.4",
		"ui-bootstrap4": "^3.0.6"
	},
	"devDependencies": {
		"csscomb": "^4.3.0",
		"husky": "^1.3.1",
		"lint-staged": "^8.2.1",
		"prettier": "1.16.1",
		"serve": "^11.3.2"
	},
	"scripts": {
		"start": "npx serve"
	},
	"repository": {
		"type": "git",
		"url": "git@github.com:JanDW/ccsp-ui.git"
	},
	"husky": {
		"hooks": {
			"pre-commit": "lint-staged"
		}
	},
	"lint-staged": {
		"components/**/*.{js,json,html}": [
			"prettier --write",
			"git add -A"
		],
		"employee-ui/**/*.{js,json,html}": [
			"prettier --write",
			"git add -A"
		],
		"components/**/*.css, employee-ui/**/*.css": [
			"csscomb --tty-mode --",
			"git add -A"
		]
	},
	"author": "Jan De Wilde",
	"license": ""
}
